
@{
    ViewBag.Title = "GetEmployees";
}

<h2>GetEmployees</h2>

<table class="table table-bordered table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>

    @foreach (var item in ViewBag.emp)//3
    {
    <tr>
        <td>@item.EmpId  </td>
        <td>@item.EmpName </td>
        <td>@item.EmpSalary</td>
    </tr>
    }

</table>

